<?xml version="1.0"?>
<robot name="na_boat">
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="1.2 0.6 0.2"/>
      </geometry>
      <material name="hull_gray">
        <color rgba="0.6 0.6 0.6 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="1.2 0.6 0.2"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="70.0"/>
      <inertia ixx="10.0" ixy="0.0" ixz="0.0" iyy="10.0" iyz="0.0" izz="10.0"/>
    </inertial>
  </link>

  <link name="hull_marker"/>
  <joint name="hull_fixed" type="fixed">
    <parent link="base_link"/>
    <child link="hull_marker"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
</robot>
